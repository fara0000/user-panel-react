{
  "compilerOptions": {
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "es2019",
      "es2020",
      "esnext",
      "dom.iterable"
    ],
    "jsx": "react",
    "module": "es6",
    "target": "esnext",
    "outDir": "./dist/",
    "strict": true,
    "allowJs": true,
    "baseUrl": "./src",
    "importHelpers": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "noUnusedParameters": true,
    "allowUnreachableCode": false,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true
  }
}